{
	"name": "lucasruroken/laravel-security",
	"description": "Security package for new laravel projects.",
	"authors": [
		{
			"name": "Guido Contreras Woda",
			"email": "gcontreras@digbang.com"
		}
	],
	"require": {
		"php": ">=5.6",
		"illuminate/support": "~4.2",
		"illuminate/routing": "~4.2",
		"illuminate/database": "~4.2",
		"illuminate/console": "~4.2",
		"illuminate/mail": "~4.2",
		"illuminate/config": "~4.2",
		"cartalyst/sentry": "~2.1",
		"doctrine/dbal": "~2.4"
	},
	"require-dev": {
		"phpspec/phpspec": "~2.0",
		"lucasruroken/laravel-doctrine":     "~2.0"
	},
	"autoload": {
		"classmap": [
			"src/migrations"
		],
		"psr-0": {
			"Digbang\\Security\\": "src/"
		}
	},
    "repositories": [
		{ "type": "vcs", "url": "git@github.com:lucasruroken/laravel-doctrine.git" }
	],
	"minimum-stability": "dev",
    "prefer-stable": true
}
